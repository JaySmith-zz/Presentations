<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <applicationSettings>
    <TransformationDemo.Properties.Settings>
      <setting name="RunEnvironment" serializeAs="String" xdt:Transform="Replace" xdt:Locator="Match(name)">
        <value>Debug</value>
      </setting>
    </TransformationDemo.Properties.Settings>
  </applicationSettings>
  
</configuration>